<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.spring.project.mappers.Mem_Receive_Mapper">
	<select id="selectJoin_id" resultType="Integer">
		select max(join_receive_no) from join_receive
	</select>


	<insert id="mem_receive_create" parameterType="Mem_ReceivceVO">
		insert into join_receive(
		join_receive_no, recruit_no, receive_date, reading, first_interview, member_no
		)
		values(
		join_receive_seq.nextval, #{recruit_no}, '2016/11/15','y','y',#{member_no}
		)
	</insert>


	<select resultType="Mem_ReceivceVO" id="listJoin">
		select * from join_receive where member_no=#{mNo}
	</select>

</mapper>  

 